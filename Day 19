
#include <stdio.h>

int main()
{
    int n,x,y,q,m;
    
    printf("Enter number of questions \n ");
    scanf("%d",&n);
    
    printf("how much marks does one question contains \n");
    scanf("%d",&x);
    
    
    printf("how many questions he got wrong \n ");
    scanf("%d",&q);
    
    printf("enter passing marks \n ");
    scanf("%d",&y);
    
    //MARKS
    
    m=(n-q)*x;
    
    if(y==m)
    {
        printf("he got marks equal to passing marks  \n");
    }
    else if(y>m) 
    {
    printf("he got marks less than passing marks  \n");
    }
    
    else
    {
        printf("he got marks greater than passing marks  \n ");
    }
}
