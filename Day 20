
#include <stdio.h>

int main()
{
    int a, b, c, d;
    
    printf("enter decision for 1st ball 1 for in and 0 for out \n") ;
    scanf("%d",&a);
    
    printf("enter decision for 2nd ball 1 for in and 0 for out \n");
    scanf("%d",&b);
    
    printf("enter decision for 3rd ball 1 for in and 0 for out \n");
    scanf("%d",&c);
    
    printf("enter decision for 4th ball 1 for in and 0 for out \n");
    scanf("%d",&d);
    
    
    if(a==1 && b==1 && c==1 && d==1)
    {
        printf("Yes IN");
    }
    else
    { 
        printf("No OUT");
}
}
