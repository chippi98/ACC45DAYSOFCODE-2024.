#include <stdio.h>

int main() 
{
    int N; 
    printf("Enter the number of terms (N): ");
    scanf("%d", &N);
    
    int coeff[N]; 
    printf("Enter the coefficients (A0, A1, ..., A(N-1)): ");
    
    for (int i = 0; i < N; i++) 
    {
        scanf("%d", &coeff[i]);
    }

    int degree = -1; 
    
    for (int i = 0; i < N; i++) 
    {
        if (coeff[i] != 0) {
            degree = i; 
        }
    }

    printf("The degree of the polynomial is: %d\n", degree);
    return 0;
}
